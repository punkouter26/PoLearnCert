{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Aspire AppHost",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build AppHost",
      "program": "${workspaceFolder}/PoLearnCert.AppHost/bin/Debug/net10.0/PoLearnCert.AppHost.dll",
      "args": [],
      "cwd": "${workspaceFolder}/PoLearnCert.AppHost",
      "stopAtEntry": false,
      "launchBrowser": {
        "enabled": true,
        "args": "http://localhost:18888",
        "windows": {
          "command": "cmd.exe",
          "args": "/C start http://localhost:18888"
        }
      },
      "env": {},
      "console": "internalConsole"
    }
  ]
}
